public class Rectangle
{
   private int length;
    public Rectangle (int radius) {
        super("Rectangle");
        this.length = length;
    }
    private int width;
    public Rectangle (int width) {
        super("Rectangle");
        this.width = width;
    }   
    @Override
    public double getArea() {
        return length * width ;
    }

    @Override
    public double getPerimeter() {
        return 2 * (length + width);
    }


}
